{
  "applicationName": "Confetch",
  "source": {
    "includePattern": ".+\\.js(doc|x)?$",   
    "include": ["./src"],                       
    "exclude": ["node_modules"]             
  },
  "sourceType": "module",
  "recurseDepth": 10,                         
  "opts": {
    "destination": "./docs/",               
    "recurse": true,
    "readme": "./README.md",
    "template": "./node_modules/@pixi/jsdoc-template",
    "package": "./package.json"        
  },
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "templates": {
    "meta": {
      "title": "Confetch",
      "description": "Configur-able/ed fetch requests",
      "keyword": "configurable fetch requests, configured fetch requests, javascript, akshay-nm, confetch"
    }
  }
}