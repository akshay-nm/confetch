{
  "source": {
    "includePattern": ".+\\.js(doc|x)?$",   
    "include": ["./src"],                       
    "exclude": ["node_modules"]             
  },
  "recurseDepth": 10,                         
  "opts": {
    "destination": "./docs/",               
    "recurse": true,
    "package": "./package.json",
    "readme": "./README.md",
    "template": "./node_modules/@pixi/jsdoc-template"        
  },
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  }
}